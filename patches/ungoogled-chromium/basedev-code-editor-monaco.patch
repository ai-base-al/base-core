+}
+
+.terminal-input-line {
+  display: flex;
+  align-items: center;
+  padding: 8px 12px;
+  border-top: 1px solid var(--border-color);
+}
+
+.prompt {
+  color: var(--success-color);
+  font-family: 'Menlo', 'Monaco', 'Courier New', monospace;
+  margin-right: 8px;
+  font-weight: bold;
+}
+
+#terminalInput {
+  flex: 1;
+  background: transparent;
