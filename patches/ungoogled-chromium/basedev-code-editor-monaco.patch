+// Base Code Editor - Main JavaScript
+
+class CodeEditor {
+  constructor() {
+    this.editor = null;
+    this.currentFile = null;
+    this.openTabs = [];
+    this.fileTree = [];
+    this.currentFolder = null;
+    this.terminalHistory = [];
+    this.historyIndex = -1;
+
+    this.init();
+  }
+
+  init() {
+    this.initMonaco();
+    this.bindEvents();
+    this.showTerminal();
+  }
+
+  initMonaco() {
+    // Load Monaco Editor from CDN
+    require.config({
+      paths: {
+        'vs': 'https://cdn.jsdelivr.net/npm/monaco-editor@0.45.0/min/vs'
+      }
+    });
+
+    require(['vs/editor/editor.main'], () => {
+      console.log('Monaco Editor loaded successfully');
+      this.createEditor();
+    });
+  }
+
+  createEditor(content = '', language = 'plaintext') {
+    const container = document.getElementById('editorContainer');
+
+    // Clear welcome screen if present
+    const welcomeScreen = container.querySelector('.welcome-screen');
+    if (welcomeScreen) {
+      welcomeScreen.remove();
+    }
+
+    // Create editor div if it doesn't exist
+    let editorDiv = document.getElementById('editor');
+    if (!editorDiv) {
+      editorDiv = document.createElement('div');
+      editorDiv.id = 'editor';
+      container.appendChild(editorDiv);
+    }
+
+    if (this.editor) {
+      this.editor.dispose();
+    }
+
+    this.editor = monaco.editor.create(editorDiv, {
+      value: content,
+      language: language,
+      theme: 'vs-dark',
+      automaticLayout: true,
+      fontSize: 13,
+      lineNumbers: 'on',
+      minimap: { enabled: true },
+      scrollBeyondLastLine: false,
+      wordWrap: 'off',
+      tabSize: 2,
+      insertSpaces: true,
+      renderWhitespace: 'selection',
+      folding: true,
+      lineDecorationsWidth: 10,
+      lineNumbersMinChars: 3
+    });
+
+    // Handle content changes
+    this.editor.onDidChangeModelContent(() => {
+      if (this.currentFile) {
+        this.markTabAsDirty(this.currentFile);
+      }
+    });
+  }
+
+  bindEvents() {
+    // Open folder buttons
+    document.getElementById('openFolder')?.addEventListener('click', () => this.openFolder());
+    document.getElementById('openFolderEmpty')?.addEventListener('click', () => this.openFolder());
+    document.getElementById('openFolderWelcome')?.addEventListener('click', () => this.openFolder());
+
+    // New file/folder buttons
+    document.getElementById('newFile')?.addEventListener('click', () => this.createNewFile());
+    document.getElementById('newFileWelcome')?.addEventListener('click', () => this.createNewFile());
+    document.getElementById('newFolder')?.addEventListener('click', () => this.createNewFolder());
+
+    // Terminal controls
+    document.getElementById('clearTerminal')?.addEventListener('click', () => this.clearTerminal());
+    document.getElementById('minimizeTerminal')?.addEventListener('click', () => this.toggleTerminal());
+
+    // Terminal input
+    const terminalInput = document.getElementById('terminalInput');
+    if (terminalInput) {
+      terminalInput.addEventListener('keydown', (e) => this.handleTerminalInput(e));
+    }
+
+    // Keyboard shortcuts
+    document.addEventListener('keydown', (e) => this.handleKeyboardShortcuts(e));
+  }
+
+  async openFolder() {
+    try {
+      // Use File System Access API
+      const dirHandle = await window.showDirectoryPicker();
+      this.currentFolder = dirHandle;
+      await this.loadFolderContents(dirHandle);
+      this.log(`Opened folder: ${dirHandle.name}`, 'success');
+    } catch (err) {
+      if (err.name !== 'AbortError') {
+        this.log(`Error opening folder: ${err.message}`, 'error');
+      }
+    }
+  }
+
+  async loadFolderContents(dirHandle, parentElement = null) {
+    const fileTreeContainer = document.getElementById('fileTree');
+
+    if (!parentElement) {
+      // Clear the file tree
+      fileTreeContainer.innerHTML = '';
+    }
+
+    const entries = [];
+    for await (const entry of dirHandle.values()) {
+      entries.push(entry);
+    }
+
+    // Sort: folders first, then files
+    entries.sort((a, b) => {
+      if (a.kind === b.kind) {
+        return a.name.localeCompare(b.name);
+      }
+      return a.kind === 'directory' ? -1 : 1;
+    });
+
+    const container = parentElement || fileTreeContainer;
+
+    for (const entry of entries) {
+      if (entry.kind === 'directory') {
+        const folderItem = this.createFolderElement(entry);
+        container.appendChild(folderItem);
+      } else {
+        const fileItem = this.createFileElement(entry);
+        container.appendChild(fileItem);
+      }
+    }
+  }
+
+  createFolderElement(dirHandle) {
+    const div = document.createElement('div');
+    div.className = 'folder-item';
+    div.innerHTML = `
+      <svg class="folder-icon" viewBox="0 0 16 16">
+        <path fill="currentColor" d="M14.5 2H7.71l-.85-.85L6.51 1h-5l-.5.5v11l.5.5h13l.5-.5v-10L14.5 2zm-.51 8.49V13h-12V2h4.49l.35.15.86.86H14v6.49z"/>
+      </svg>
+      <span>${dirHandle.name}</span>
+    `;
+
+    div.addEventListener('click', async (e) => {
+      e.stopPropagation();
+      // TODO: Expand/collapse folder
+      this.log(`Folder: ${dirHandle.name}`, 'success');
+    });
+
+    return div;
+  }
+
+  createFileElement(fileHandle) {
+    const div = document.createElement('div');
+    div.className = 'file-item';
+    const icon = this.getFileIcon(fileHandle.name);
+    div.innerHTML = `
+      ${icon}
+      <span>${fileHandle.name}</span>
+    `;
+
+    div.addEventListener('click', async (e) => {
+      e.stopPropagation();
+      await this.openFile(fileHandle);
+    });
+
+    return div;
+  }
+
+  async openFile(fileHandle) {
+    try {
+      const file = await fileHandle.getFile();
+      const content = await file.text();
+      const language = this.getLanguageFromFilename(file.name);
+
+      // Create editor if it doesn't exist
+      if (!this.editor) {
+        this.createEditor(content, language);
+      } else {
+        this.editor.setValue(content);
+        monaco.editor.setModelLanguage(this.editor.getModel(), language);
+      }
+
+      this.currentFile = {
+        handle: fileHandle,
+        name: file.name,
+        content: content,
+        dirty: false
+      };
+
+      this.addTab(file.name);
+      this.log(`Opened: ${file.name}`, 'success');
+    } catch (err) {
+      this.log(`Error opening file: ${err.message}`, 'error');
+    }
+  }
+
+  addTab(filename) {
+    const tabBar = document.getElementById('tabs');
+
+    // Check if tab already exists
+    const existingTab = Array.from(tabBar.children).find(
+      tab => tab.dataset.filename === filename
+    );
+
+    if (existingTab) {
+      this.activateTab(existingTab);
+      return;
+    }
+
+    const tab = document.createElement('div');
+    tab.className = 'tab active';
+    tab.dataset.filename = filename;
+    tab.innerHTML = `
+      <span class="tab-label">${filename}</span>
+      <span class="tab-close">×</span>
+    `;
+
+    // Deactivate other tabs
+    Array.from(tabBar.children).forEach(t => t.classList.remove('active'));
+
+    tab.querySelector('.tab-close').addEventListener('click', (e) => {
+      e.stopPropagation();
+      this.closeTab(tab);
+    });
+
+    tab.addEventListener('click', () => this.activateTab(tab));
+
+    tabBar.appendChild(tab);
+    this.openTabs.push({ filename, element: tab });
+  }
+
+  activateTab(tab) {
+    const tabBar = document.getElementById('tabs');
+    Array.from(tabBar.children).forEach(t => t.classList.remove('active'));
+    tab.classList.add('active');
+    // TODO: Load file content when switching tabs
+  }
+
+  closeTab(tab) {
+    const filename = tab.dataset.filename;
+    this.openTabs = this.openTabs.filter(t => t.filename !== filename);
+    tab.remove();
+
+    if (this.openTabs.length === 0) {
+      // Show welcome screen again
+      this.showWelcomeScreen();
+    }
+  }
+
+  markTabAsDirty(file) {
+    const tab = this.openTabs.find(t => t.filename === file.name);
+    if (tab && !tab.dirty) {
+      tab.element.querySelector('.tab-label').textContent = `${file.name} •`;
+      tab.dirty = true;
+    }
+  }
+
+  async saveCurrentFile() {
+    if (!this.currentFile || !this.editor) {
+      this.log('No file to save', 'error');
+      return;
+    }
+
+    try {
+      const content = this.editor.getValue();
+      const writable = await this.currentFile.handle.createWritable();
+      await writable.write(content);
+      await writable.close();
+
+      this.currentFile.dirty = false;
+      this.log(`Saved: ${this.currentFile.name}`, 'success');
+
+      // Update tab
+      const tab = this.openTabs.find(t => t.filename === this.currentFile.name);
+      if (tab) {
+        tab.element.querySelector('.tab-label').textContent = this.currentFile.name;
+        tab.dirty = false;
+      }
+    } catch (err) {
+      this.log(`Error saving file: ${err.message}`, 'error');
+    }
+  }
+
+  createNewFile() {
+    const filename = prompt('Enter filename:');
+    if (!filename) return;
+
+    if (!this.editor) {
+      this.createEditor('', this.getLanguageFromFilename(filename));
+    } else {
+      this.editor.setValue('');
+    }
+
+    this.currentFile = {
+      handle: null,
+      name: filename,
+      content: '',
+      dirty: true
+    };
+
+    this.addTab(filename);
+    this.log(`Created: ${filename}`, 'success');
+  }
+
+  createNewFolder() {
+    this.log('Create new folder - coming soon', 'success');
+  }
+
+  showWelcomeScreen() {
+    const container = document.getElementById('editorContainer');
+    const editorDiv = document.getElementById('editor');
+    if (editorDiv) {
+      editorDiv.remove();
+    }
+    if (this.editor) {
+      this.editor.dispose();
+      this.editor = null;
+    }
+
+    container.innerHTML = `
+      <div class="welcome-screen">
+        <h1>Base Code Editor</h1>
+        <p>A powerful code editor built into Base Browser</p>
+        <div class="quick-actions">
+          <button id="openFolderWelcome" class="primary-btn">Open Folder</button>
+          <button id="newFileWelcome" class="secondary-btn">New File</button>
+        </div>
+        <div class="features">
+          <h3>Features</h3>
+          <ul>
+            <li>Syntax highlighting for 100+ languages</li>
+            <li>Integrated file explorer</li>
+            <li>Built-in terminal</li>
+            <li>Multi-tab editing</li>
+            <li>Real-time file watching</li>
+          </ul>
+        </div>
+      </div>
+    `;
+
+    // Re-bind events for welcome screen buttons
+    this.bindEvents();
+  }
+
+  // Terminal functions
+  showTerminal() {
+    const terminal = document.getElementById('terminal');
+    terminal.classList.remove('hidden');
+    this.log('Terminal ready. Type "help" for available commands.', 'success');
+  }
+
+  toggleTerminal() {
+    const terminal = document.getElementById('terminal');
+    terminal.classList.toggle('minimized');
+  }
+
+  clearTerminal() {
+    const output = document.getElementById('terminalOutput');
+    output.innerHTML = '';
+  }
+
+  handleTerminalInput(e) {
+    if (e.key === 'Enter') {
+      const input = e.target.value.trim();
+      if (input) {
+        this.executeCommand(input);
+        this.terminalHistory.push(input);
+        this.historyIndex = this.terminalHistory.length;
+        e.target.value = '';
+      }
+    } else if (e.key === 'ArrowUp') {
+      e.preventDefault();
+      if (this.historyIndex > 0) {
+        this.historyIndex--;
+        e.target.value = this.terminalHistory[this.historyIndex];
+      }
+    } else if (e.key === 'ArrowDown') {
+      e.preventDefault();
+      if (this.historyIndex < this.terminalHistory.length - 1) {
+        this.historyIndex++;
+        e.target.value = this.terminalHistory[this.historyIndex];
+      } else {
+        this.historyIndex = this.terminalHistory.length;
+        e.target.value = '';
+      }
+    }
+  }
+
+  executeCommand(cmd) {
+    this.log(`$ ${cmd}`);
+
+    const parts = cmd.split(' ');
+    const command = parts[0];
+
+    switch (command) {
+      case 'help':
+        this.log('Available commands:', 'success');
+        this.log('  help     - Show this help message');
+        this.log('  clear    - Clear terminal');
+        this.log('  ls       - List files (simulated)');
+        this.log('  pwd      - Print working directory');
+        this.log('  echo     - Echo text');
+        break;
+
+      case 'clear':
+        this.clearTerminal();
+        break;
+
+      case 'ls':
+        if (this.currentFolder) {
+          this.log('Files in current folder:', 'success');
+          this.log('(Feature coming soon - will show actual files)');
+        } else {
+          this.log('No folder opened', 'error');
+        }
+        break;
+
+      case 'pwd':
+        if (this.currentFolder) {
+          this.log(this.currentFolder.name, 'success');
+        } else {
+          this.log('No folder opened', 'error');
+        }
+        break;
+
+      case 'echo':
+        this.log(parts.slice(1).join(' '), 'success');
+        break;
+
+      default:
+        this.log(`Command not found: ${command}`, 'error');
+        this.log('Type "help" for available commands');
+    }
+  }
+
+  log(message, type = 'info') {
+    const output = document.getElementById('terminalOutput');
+    const line = document.createElement('div');
+    line.className = `terminal-line ${type === 'error' ? 'terminal-error' : ''} ${type === 'success' ? 'terminal-success' : ''}`;
+    line.textContent = message;
+    output.appendChild(line);
+    output.scrollTop = output.scrollHeight;
+  }
+
+  handleKeyboardShortcuts(e) {
+    // Cmd/Ctrl + S: Save
+    if ((e.metaKey || e.ctrlKey) && e.key === 's') {
+      e.preventDefault();
+      this.saveCurrentFile();
+    }
+
+    // Cmd/Ctrl + O: Open folder
+    if ((e.metaKey || e.ctrlKey) && e.key === 'o') {
+      e.preventDefault();
+      this.openFolder();
+    }
+
+    // Cmd/Ctrl + N: New file
+    if ((e.metaKey || e.ctrlKey) && e.key === 'n') {
+      e.preventDefault();
+      this.createNewFile();
+    }
+
+    // Cmd/Ctrl + `: Toggle terminal
+    if ((e.metaKey || e.ctrlKey) && e.key === '`') {
+      e.preventDefault();
+      this.toggleTerminal();
+    }
+  }
+
+  getLanguageFromFilename(filename) {
+    const ext = filename.split('.').pop().toLowerCase();
+    const languageMap = {
+      'js': 'javascript',
+      'jsx': 'javascript',
+      'ts': 'typescript',
+      'tsx': 'typescript',
+      'py': 'python',
+      'rb': 'ruby',
+      'java': 'java',
+      'cpp': 'cpp',
+      'c': 'c',
+      'h': 'cpp',
+      'hpp': 'cpp',
+      'cs': 'csharp',
+      'go': 'go',
+      'rs': 'rust',
+      'php': 'php',
+      'html': 'html',
+      'htm': 'html',
+      'css': 'css',
+      'scss': 'scss',
+      'sass': 'sass',
+      'less': 'less',
+      'json': 'json',
+      'xml': 'xml',
+      'yaml': 'yaml',
+      'yml': 'yaml',
+      'md': 'markdown',
+      'sql': 'sql',
+      'sh': 'shell',
+      'bash': 'shell',
+      'zsh': 'shell',
+      'ps1': 'powershell',
+      'vue': 'vue',
+      'svelte': 'svelte'
+    };
+
+    return languageMap[ext] || 'plaintext';
+  }
+
+  getFileIcon(filename) {
+    // Simple file icon based on extension
+    return `
+      <svg class="file-icon" viewBox="0 0 16 16">
+        <path fill="currentColor" d="M13.5 0h-12l-.5.5v15l.5.5h12l.5-.5v-15l-.5-.5zM13 15H2V1h11v14z"/>
+      </svg>
+    `;
+  }
+}
+
+// Initialize the editor when DOM is ready
+if (document.readyState === 'loading') {
+  document.addEventListener('DOMContentLoaded', () => {
+    window.codeEditor = new CodeEditor();
+  });
+} else {
+  window.codeEditor = new CodeEditor();
+}
